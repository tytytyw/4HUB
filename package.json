{
  "name": "4hab-frontend",
  "version": "0.1.0",
  "proxy": "https://fs2.mh.net.ua",
  "dependencies": {
    "@fullcalendar/daygrid": "^5.8.0",
    "@fullcalendar/interaction": "^5.8.0",
    "@fullcalendar/react": "^5.8.0",
    "@fullcalendar/timegrid": "^5.8.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.4",
    "canvas-txt": "^3.0.0",
    "chart.js": "^3.3.2",
    "classnames": "^2.2.6",
    "emoji-picker-react": "^3.4.8",
    "html2canvas": "^1.3.2",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-chartjs-2": "^3.0.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.1",
    "react-flow-renderer": "^9.6.9",
    "react-image-crop": "^10.0.0",
    "react-moment": "^1.1.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject",
    "deploy": "npm run lint:fix && npm run convert-po && npm run build && gulp deploy",
    "publish": "gulp publish",
    "extract:lang": "react-localized-extractor -l en -a.gettext __ -a.ngettext __n -o './translations' && react-localized-extractor -l uk -a.gettext __ -a.ngettext __n -o './translations'",
    "convert-po": "node ./translations/po2json.js",
    "lint:fix": "eslint . --fix",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "del": "^6.0.0",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-prettier-react": "0.0.24",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^1.7.0",
    "ftp-deploy": "^2.4.1",
    "gettext-parser": "^5.1.1",
    "gulp": "^4.0.2",
    "gulp-git": "^2.10.1",
    "jest-css-modules": "^2.1.0",
    "prettier": "^1.19.1",
    "react-localized": "^3.2.0",
    "react-localized-extractor": "^2.1.0"
  },
  "jest": {
    "clearMocks": false,
    "verbose": true,
    "collectCoverageFrom": [
      "src/generalComponents/**/*.{js|jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl|sass)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  }
}
